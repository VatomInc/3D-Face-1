{
  "name": "@vatom/3d-face",
  "version": "4.0.1",
  "description": "This vAtom face can plug into the SDKs to render 3D content in either binary glTF or V3D format.",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-lib && npm run copy-webapp && npm run copy-android",
    "build-lib": "webpack -p",
    "copy-webapp": "cpy lib/**/* webapp/",
    "copy-android": "cpy webapp/**/* android/face3d/src/main/assets/face3drenderer/"
  },
  "keywords": [
    "3d",
    "renderer",
    "v3d",
    "gltf",
    "vatom",
    "face"
  ],
  "author": "jjv360",
  "license": "ISC",
  "dependencies": {
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.20",
    "three": "^0.140.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-loader": "^8.0.6",
    "cpy-cli": "^3.0.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
